#bubble sort  using for loop
arr=[1,2,8,6,5]
def Bubble_sort(array):
    for i in range (len(array)):
        for j in range(len(array)-1):
            if array[j]>array[j+1]:
                temp=array[j]
                array[j]=array[j+1]
                array[j+1]=temp
    for i in range(len(array)):
        print(array[i])
Bubble_sort(arr)
        
    
    
    
